# General tweaks
alias bc="bc -l"
alias date="date --iso-8601=seconds"
alias df="df -H"
alias go="xdg-open"
alias ls="ls --color=auto -Gh"
alias ps="ps -Ac"
alias rm="rm -i"
alias hide="set +o history; echo hidden"

# Programming Help
alias ap8="autopep8 --aggressive --recursive --in-place --verbose --ignore=W391"
alias clean="~/bin/essentials/clean.py"
alias f8="python3 -m flake8 --ignore=W391"
alias jn="jupyter notebook"
